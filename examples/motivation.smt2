(set-logic UFLIA)
(declare-fun f (Int Int) Int)
(declare-fun g (Int) Int)
(declare-const a Int)
(declare-const b Int)
(assert
  (>= (f 3 a)
      (+ 4 (g b)))
)
(assert
  (forall ((x Int) (y Int))
    (< (f x y)
       (+ x (g y)))
    )
)
(assert
  (forall ((x Int))
    (= (g x)
       (g (+ x 2)))
    )
)
(check-sat)
